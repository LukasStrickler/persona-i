{
  "name": "persona-questionnaire",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "eslint . --ext .ts,.tsx && tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:setup": "dotenv -e .env -- bunx tsx scripts/setup-db.ts",
    "db:setup:full": "dotenv -e .env -- bunx tsx scripts/setup-db.ts --full",
    "dev": "next dev --turbo",
    "email": "email dev -d src/emails -p 4001",
    "format:check": "bunx prettier --check \"**/*.{ts,tsx,js,jsx,md,mdx}\" --cache",
    "format:write": "bunx prettier --write \"**/*.{ts,tsx,js,jsx,md,mdx}\" --cache",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "agent:finalize": "bash scripts/finalize.sh agent",
    "ci:finalize": "bash scripts/finalize.sh ci",
    "docs:check": "bash scripts/check-docs.sh",
    "review:task": "bash scripts/review-run.sh task",
    "review:pr": "bash scripts/review-run.sh pr",
    "review:read": "bash scripts/review-read.sh",
    "review:cleanup": "bash scripts/review-cleanup.sh",
    "pr:comments": "bash scripts/pr-comments-fetch.sh read",
    "pr:comments:detect": "bash scripts/pr-comments-detect.sh",
    "pr:comments:get": "bash scripts/pr-comments-get.sh",
    "pr:comments:resolve": "bash scripts/pr-comments-resolve.sh",
    "pr:comments:resolve:interactive": "bash scripts/pr-comments-fetch.sh resolve",
    "pr:comments:dismiss": "bash scripts/pr-comments-dismiss.sh",
    "pr:comments:cleanup": "bash scripts/pr-comments-cleanup.sh --all",
    "pr:list": "bash scripts/pr-comments-fetch.sh list"
  },
  "dependencies": {
    "@hcaptcha/react-hcaptcha": "^1.14.0",
    "@hookform/resolvers": "^5.2.2",
    "@libsql/client": "^0.15.15",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
